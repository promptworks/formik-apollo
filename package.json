{
  "version": "2.0.3",
  "name": "formik-apollo",
  "repository": "https://github.com/promptworks/formik-apollo",
  "description": "A React hook to extract validation errors from a GraphQL request and apply them to a Formik form.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "bili --format cjs --format esm src/index.ts",
    "clean": "rm -rf dist",
    "format": "prettier --write '**/*.{js,ts,tsx}'",
    "format:check": "prettier --check '**/*.{js,ts,tsx}'",
    "prepublishOnly": "yarn build"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "peerDependencies": {
    "apollo-client": "*",
    "formik": "^2.0",
    "graphql": "*",
    "react": ">=16.8"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.1.0",
    "@types/jest": "^25.2.1",
    "apollo-client": "^2.6.4",
    "bili": "^4.8.1",
    "formik": "2.1.4",
    "graphql": "^15.0.0",
    "jest": "^25.4.0",
    "prettier": "^2.3.0",
    "react": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.6.4"
  }
}
